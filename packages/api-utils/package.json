{
    "name": "@igor-siergiej/api-utils",
    "version": "0.0.2",
    "description": "Shared API utilities for Node services",
    "author": "igor-siergiej",
    "license": "AGPL-3.0 license",
    "repository": {
        "type": "git",
        "url": "https://github.com/igor-siergiej/im-apps-utils.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "main": "build/index.cjs",
    "module": "build/index.js",
    "types": "build/index.d.ts",
    "exports": {
        ".": {
            "import": "./build/index.js",
            "require": "./build/index.cjs",
            "types": "./build/index.d.ts"
        }
    },
    "files": [
        "build"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rimraf build",
        "prepack": "yarn clean && yarn build"
    },
    "sideEffects": false,
    "devDependencies": {
        "rimraf": "^6.0.1",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    }
}
