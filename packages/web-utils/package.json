{
    "name": "@igor-siergiej/web-utils",
    "version": "0.0.6",
    "description": "Shared web utilities for frontend apps",
    "author": "igor-siergiej",
    "license": "AGPL-3.0 license",
    "repository": {
        "type": "git",
        "url": "https://github.com/igor-siergiej/im-apps-utils.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "main": "build/index.cjs",
    "module": "build/index.js",
    "types": "build/index.d.ts",
    "exports": {
        ".": {
            "types": "./build/index.d.ts",
            "import": "./build/index.js",
            "require": "./build/index.cjs"
        }
    },
    "files": [
        "build"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rimraf build",
        "prepack": "yarn clean && yarn build"
    },
    "sideEffects": false,
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-router-dom": ">=6.0.0"
    },
    "devDependencies": {
        "@types/react": "^19.1.13",
        "react": "^19.1.1",
        "react-router-dom": "^7.9.1",
        "rimraf": "^6.0.1",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    }
}
